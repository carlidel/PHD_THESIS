@inproceedings{Turchetti:1990aa,
  doi       = {10.1007/978-3-642-75405-0_24},
  title     = {Nekhoroshev Stability Estimates for Symplectic Maps and Physical Applications},
  author    = {Turchetti, G.},
  year      = 1990,
  booktitle = {Number Theory and Physics},
  publisher = {Springer Berlin Heidelberg},
  address   = {Berlin, Heidelberg},
  series    = {Springer Proceedings in Physics},
  volume    = 47,
  pages     = {223--234},
  isbn      = {978-3-642-75405-0},
  editor    = {Luck, Jean-Marc and Moussa, Pierre and Waldschmidt, Michel},
  abstract  = {The Nekhoroshev stability estimates for symplectic maps of R2dare presented. After an illustration of the basic ideas in a simple hamiltonian model, the dynamics of a charged particle in the magnetic lattice of an accelerator is considered and it is shown to be conveniently described by a symplectic map. After recalling the basic properties of the Birkhoff normal forms, we state a general theorem on the stability of the orbits and sketch the proof. The factorial divergence of the Birkhoff series appears to arise from nonlinearity as well as from the divisors, and a functional equation with interesting analytic properties is obtained from the majorant series.}
}

